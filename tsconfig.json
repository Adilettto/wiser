{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@":["/"],
      "Pages/*":["Pages/*"],
      "Components/*":["Components/*"],
      "Service/*":["Service/*"],
      "Shared/*":["Shared/*"],
      "Redux/*":["Redux/*"],
      "Routes/*": ["Routes/*"],
      "Styles/*": ["Styles/*"],
      "Assets/*": ["Assets/*"],
      "Containers/*": ["Containers/*"],
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
